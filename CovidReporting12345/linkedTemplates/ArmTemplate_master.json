{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "CovidReporting12345"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportingdlsk12.dfs.core.windows.net/"
		},
		"ls_http_openECDCdata_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ecdc.blob.core.windows.net"
		},
		"trigger1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/0822b12b-3c52-4cee-82c1-763a70b4dbf9/resourceGroups/CovidReporting/providers/Microsoft.Storage/storageAccounts/reportingcovidstorage"
		},
		"trigger_cases_death_pipeline_properties_pl_ingest_ecds_data_parameters_sourceRelativeURL": {
			"type": "string",
			"defaultValue": "covid19/cases_deaths.csv"
		},
		"trigger_cases_death_pipeline_properties_pl_ingest_ecds_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "cases_deaths.csv"
		},
		"trigger_hospital_admission_properties_pl_ingest_ecds_data_parameters_sourceRelativeURL": {
			"type": "string",
			"defaultValue": "covid19/hospital_admissions.csv"
		},
		"trigger_hospital_admission_properties_pl_ingest_ecds_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "hospital_admissions.csv"
		},
		"trigger_testing_pipeline_properties_pl_ingest_ecds_data_parameters_sourceRelativeURL": {
			"type": "string",
			"defaultValue": "covid19/testing.csv"
		},
		"trigger_testing_pipeline_properties_pl_ingest_ecds_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "testing.csv"
		},
		"triggercase_deaths_uk_ind_only_pipeline_properties_pl_ingest_ecds_data_parameters_sourceRelativeURL": {
			"type": "string",
			"defaultValue": "covid19/case_deaths_uk_ind_only.csv"
		},
		"triggercase_deaths_uk_ind_only_pipeline_properties_pl_ingest_ecds_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "case_deaths_uk_ind_only.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "CovidReporting12345_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"ls_http_openECDCdata_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_openECDCdata_eu_properties_typeProperties_url')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					},
					"trigger_cases_death_pipeline_properties_pl_ingest_ecds_data_parameters_sourceRelativeURL": {
						"value": "[parameters('trigger_cases_death_pipeline_properties_pl_ingest_ecds_data_parameters_sourceRelativeURL')]"
					},
					"trigger_cases_death_pipeline_properties_pl_ingest_ecds_data_parameters_sinkFileName": {
						"value": "[parameters('trigger_cases_death_pipeline_properties_pl_ingest_ecds_data_parameters_sinkFileName')]"
					},
					"trigger_hospital_admission_properties_pl_ingest_ecds_data_parameters_sourceRelativeURL": {
						"value": "[parameters('trigger_hospital_admission_properties_pl_ingest_ecds_data_parameters_sourceRelativeURL')]"
					},
					"trigger_hospital_admission_properties_pl_ingest_ecds_data_parameters_sinkFileName": {
						"value": "[parameters('trigger_hospital_admission_properties_pl_ingest_ecds_data_parameters_sinkFileName')]"
					},
					"trigger_testing_pipeline_properties_pl_ingest_ecds_data_parameters_sourceRelativeURL": {
						"value": "[parameters('trigger_testing_pipeline_properties_pl_ingest_ecds_data_parameters_sourceRelativeURL')]"
					},
					"trigger_testing_pipeline_properties_pl_ingest_ecds_data_parameters_sinkFileName": {
						"value": "[parameters('trigger_testing_pipeline_properties_pl_ingest_ecds_data_parameters_sinkFileName')]"
					},
					"triggercase_deaths_uk_ind_only_pipeline_properties_pl_ingest_ecds_data_parameters_sourceRelativeURL": {
						"value": "[parameters('triggercase_deaths_uk_ind_only_pipeline_properties_pl_ingest_ecds_data_parameters_sourceRelativeURL')]"
					},
					"triggercase_deaths_uk_ind_only_pipeline_properties_pl_ingest_ecds_data_parameters_sinkFileName": {
						"value": "[parameters('triggercase_deaths_uk_ind_only_pipeline_properties_pl_ingest_ecds_data_parameters_sinkFileName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}